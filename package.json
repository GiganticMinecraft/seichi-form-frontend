{
  "name": "seichi-portal-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all -p dev:*",
    "dev:mock": "prism mock openapi/schema/openapi.yml -p 3001",
    "dev:next": "next dev",
    "build": "next build && next export && next-export-optimize-images",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --check --loglevel=warn 'src/**/*.{js,jsx,ts,tsx,html,json}'",
    "format:fix": "prettier --write --loglevel=warn 'src/**/*.{js,jsx,ts,tsx,html,json}'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --testLocationInResults --ci",
    "postinstall": "typesync || :",
    "prepare": "husky install",
    "sb": "start-storybook -p 9001",
    "sb:start": "http-server -s -p 9001 storybook-static",
    "sb:build": "build-storybook",
    "sb:test": "yarn sb:build && npm-run-all -p -r sb:start sb:test:run",
    "sb:test:run": "test-storybook --url http://localhost:9001 --coverage",
    "api": "redocly bundle openapi/schema/openapi.yml -o src/libs/openapi.yml && rm -rf src/api && openapi2aspida"
  },
  "dependencies": {
    "@azure/msal-browser": "^2.33.0",
    "@azure/msal-react": "^1.5.3",
    "@chakra-ui/react": "2.5.1",
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@tanstack/react-query": "4.24.10",
    "@tanstack/react-query-devtools": "4.24.14",
    "framer-motion": "10.0.1",
    "jsonwebtoken": "8.5.1",
    "next": "13.2.3",
    "option-t": "^35.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.43.4",
    "react-icons": "4.8.0",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@chakra-ui/storybook-addon": "^4.0.13",
    "@redocly/cli": "^1.0.0-beta.117",
    "@stoplight/prism-cli": "^4.10.5",
    "@storybook/addon-a11y": "^6.5.13",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-coverage": "^0.0.8",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/addon-storyshots": "^6.5.13",
    "@storybook/builder-webpack5": "^6.5.13",
    "@storybook/jest": "^0.0.10",
    "@storybook/manager-webpack5": "^6.5.13",
    "@storybook/react": "^6.5.13",
    "@storybook/test-runner": "^0.9.4",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/babel__core": "^7.1.20",
    "@types/eslint": "^8.4.10",
    "@types/http-server": "^0.12.1",
    "@types/jsonwebtoken": "9.0.1",
    "@types/node": "^18.11.9",
    "@types/prettier": "^2.7.1",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "babel-loader": "^9.0.0",
    "eslint": "^8.28.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^13.0.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-storybook": "^0.6.7",
    "eslint-plugin-testing-library": "^5.9.1",
    "http-server": "^14.1.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "^13.0.4",
    "next-export-optimize-images": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "openapi2aspida": "^0.21.0",
    "prettier": "^2.8.0",
    "storybook-addon-next": "^1.7.2",
    "stream-browserify": "^3.0.0",
    "typescript": "4.9.5",
    "typesync": "^0.10.0"
  }
}
