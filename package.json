{
  "name": "seichi-portal-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export && next-export-optimize-images",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --check --loglevel=warn 'src/**/*.{js,jsx,ts,tsx,html,json}'",
    "format:fix": "prettier --write --loglevel=warn 'src/**/*.{js,jsx,ts,tsx,html,json}'",
    "test": "jest",
    "test:watch": "jest --watch",
    "postinstall": "typesync || :",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "api": "redocly bundle openapi/schema/openapi.yml -o src/libs/openapi.yml && rm -rf src/api && openapi2aspida"
  },
  "dependencies": {
    "@aspida/axios": "1.11.0",
