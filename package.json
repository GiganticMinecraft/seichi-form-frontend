    "axios": "1.2.1",
    "camelcase-keys": "8.0.2",
    "framer-motion": "7.10.2",
    "ky": "0.33.0",
    "next": "13.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.41.0",
    "react-icons": "4.7.1",
